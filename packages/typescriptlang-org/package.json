{
  "name": "typescriptlang-org",
  "private": true,
  "description": "The TypeScript Website",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build",
    "clean": "gatsby clean",
